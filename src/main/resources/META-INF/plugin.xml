<idea-plugin>
    <id>maven-versions-updater-plugin</id>
    <name>Maven Updater</name>
    <vendor email="cs.liaow@gmail.com" url="https://github.com/saharag/maven-versions-updater-plugin">Sahara Team
    </vendor>
    <version><![CDATA[ version will be added by gradle build]]></version>

    <idea-version since-build="190.00"/>

    <description><![CDATA[ Description will be added by gradle build]]></description>

    <category>Tools Integration</category>

    <!-- dependencies -->
    <depends>org.jetbrains.idea.maven</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- inspections -->
        <localInspection language="XML"
                         shortName="MavenVersionsUpdater"
                         bundle="messages.MavenVersionUpdateBundle"
                         key="inspection.version.updater.name"
                         enabledByDefault="true"
                         level="WARNING"
                         implementationClass="com.sahara.intellij.plugin.mvn.MavenDependenciesVersionUpdateInspection"
                         groupKey="inspection.group"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>

    <change-notes><![CDATA[	Change notes will be filled by gradle build ]]>
    </change-notes>
</idea-plugin>
